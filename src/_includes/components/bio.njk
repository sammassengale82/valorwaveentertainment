<section id="bio">
  <h2>Meet the DJ</h2>
  <div class="bio-wrap">
    <div class="bio-head">
      <img class="bio-image" src="{{ home.bio_image }}" alt="{{ home.bio_name }}" />
      <div class="bio-name">{{ home.bio_name }}</div>
    </div>
    {% for paragraph in home.bio_paragraphs %}
      <p>{{ paragraph | safe }}</p>
    {% endfor %}
  </div>
</section>